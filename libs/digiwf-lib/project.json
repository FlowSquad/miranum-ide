{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/digiwf-lib/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/digiwf-lib/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/digiwf-lib"],
      "options": {
        "jestConfig": "libs/digiwf-lib/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/digiwf-lib",
        "tsConfig": "libs/digiwf-lib/tsconfig.lib.json",
        "packageJson": "libs/digiwf-lib/package.json",
        "main": "libs/digiwf-lib/src/index.ts",
        "assets": [
          "libs/digiwf-lib/*.md",
          "resources/"
        ]
      }
    },
    "publish": {
      "executor": "@nrwl/js:tsc"
    },
    "vscode:prepublish": {
      "executor": "@nrwl/js:tsc"
    }
  },
  "tags": []
}
