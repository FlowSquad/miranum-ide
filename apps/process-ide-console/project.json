{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/process-ide-console/src",
  "projectType": "application",
  "implicitDependencies": ["process-ide-console-webview"],
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/process-ide-console",
        "deleteOutputPath": false,
        "main": "apps/process-ide-console/src/main.ts",
        "tsConfig": "apps/process-ide-console/tsconfig.app.json",
        "generatePackageJson": true,
        "webpackConfig": "apps/process-ide-console/custom-webpack.config.js",
        "assets": [
          "apps/process-ide-console/src/assets/komet.svg",
          {
            "glob": "LICENSE",
            "input": "./",
            "output": "/"
          }
        ]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/process-ide-console/src/environments/environment.ts",
              "with": "apps/process-ide-console/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "process-ide-console:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "process-ide-console:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/process-ide-console/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/process-ide-console"],
      "options": {
        "jestConfig": "apps/process-ide-console/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
